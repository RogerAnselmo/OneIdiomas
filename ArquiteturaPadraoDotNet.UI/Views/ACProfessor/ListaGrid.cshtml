@model ICollection<ACProfessorViewModel>
@{
    Layout = null;
}

<table id="tableProfessor" class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>
                Professor
            </th>
            <th>
                CPF
            </th>
            <th>Ações</th>
        </tr>
    </thead>
    <tbody>
        @foreach (ACProfessorViewModel Professor in Model)
        {
            <tr>
                <td>
                    @Professor.NomeCompleto
                </td>
                <td>
                    @Professor.CPF
                </td>
                <td>
                    <a href="" class="btn btn-editar btn-default" title="Visualizar"><i class="glyphicon glyphicon-eye-open"></i></a>
                    <a href="@Url.Action("Cadastro-Professor", "Gerenciar-Professor",new { id = Professor.CodigoProfessor })" class="btn btn-editar btn-primary" title="Editar"><i class="glyphicon glyphicon-edit"></i></a>
                    <a class="btn btn-editar btn-danger" title="Excluir" onclick="ExcluirProfessor(@Professor.CodigoProfessor, this)"><i class="glyphicon glyphicon-trash"></i></a>
                </td>
            </tr>
        }
    </tbody>
</table>
<script src="~/CoreJs/ACProfessor/Lista/Grid.js"></script>